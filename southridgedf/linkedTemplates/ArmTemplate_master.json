{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "southridgedf"
		},
		"CloudSales_AzSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSales_AzSQLDB'"
		},
		"CloudStreaming_AzSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudStreaming_AzSQLDB'"
		},
		"FourthCoffeeVM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FourthCoffeeVM'"
		},
		"Southridge_CosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Southridge_CosmosDb'"
		},
		"Southridge_DataLake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Southridge_DataLake'"
		},
		"VanArsdel_SQLVM_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'VanArsdel_SQLVM'"
		},
		"FourthCoffeeVM_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Rentals"
		},
		"FourthCoffeeVM_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "southridge"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "southridgedf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CloudSales_AzSQLDB_connectionString": {
						"value": "[parameters('CloudSales_AzSQLDB_connectionString')]"
					},
					"CloudStreaming_AzSQLDB_connectionString": {
						"value": "[parameters('CloudStreaming_AzSQLDB_connectionString')]"
					},
					"Southridge_CosmosDb_connectionString": {
						"value": "[parameters('Southridge_CosmosDb_connectionString')]"
					},
					"Southridge_DataLake_connectionString": {
						"value": "[parameters('Southridge_DataLake_connectionString')]"
					},
					"FourthCoffeeVM_password": {
						"value": "[parameters('FourthCoffeeVM_password')]"
					},
					"VanArsdel_SQLVM_connectionString": {
						"value": "[parameters('VanArsdel_SQLVM_connectionString')]"
					},
					"FourthCoffeeVM_properties_typeProperties_host": {
						"value": "[parameters('FourthCoffeeVM_properties_typeProperties_host')]"
					},
					"FourthCoffeeVM_properties_typeProperties_userId": {
						"value": "[parameters('FourthCoffeeVM_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "southridgedf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'southridgedf_ArmTemplate_0')]"
			]
		}
	]
}